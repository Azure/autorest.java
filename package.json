{
  "name": "@autorest/java",
  "version": "4.1.5",
  "description": "The Java extension for classic generators in AutoRest.",
  "scripts": {
    "autorest": "autorest",
    "start": "java -jar javagen/target/azure-autorest-javagen-jar-with-dependencies.jar",
    "debug": "java -agentlib:jdwp=transport=dt_socket,server=y,suspend=y,quiet=y,address=5005 -jar javagen/target/azure-autorest-javagen-jar-with-dependencies.jar",
    "build": "mvn package -P local",
    "generate": "./generate",
    "testserver-run": "autorest-testserver run",
    "testserver-stop": "autorest-testserver stop",
    "testserver-coverage-publish": "autorest-testserver-coverage publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/autorest.java"
  },
  "readme": "./readme.md",
  "keywords": [
    "autorest",
    "java"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest.java/issues"
  },
  "homepage": "https://github.com/Azure/autorest.java/blob/main/readme.md",
  "files": [
    "*/package.json",
    "/readme.md",
    "/androidgen/*.md",
    "/fluentgen/*.md",
    "/fluentnamer/*.md",
    "/javagen/*.md",
    "/postprocessor/*.md",
    "/preprocessor/*.md",
    "*/target/*-jar-with-dependencies.jar",
    "/postprocessor/jdt-language-server/config_linux/*.ini",
    "/postprocessor/jdt-language-server/config_mac/*.ini",
    "/postprocessor/jdt-language-server/config_ss_linux/*.ini",
    "/postprocessor/jdt-language-server/config_ss_mac/*.ini",
    "/postprocessor/jdt-language-server/config_ss_win/*.ini",
    "/postprocessor/jdt-language-server/config_win/*.ini",
    "/postprocessor/jdt-language-server/features",
    "/postprocessor/jdt-language-server/plugins"
  ],
  "devDependencies": {
    "autorest": "^3.0.0",
    "@microsoft.azure/autorest.testserver": "3.3.34"
  }
}
