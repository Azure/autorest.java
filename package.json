{
  "name": "@microsoft.azure/autorest.java",
  "version": "3.0.0",
  "description": "The Java extension for classic generators in AutoRest.",
  "scripts": {
    "autorest": "autorest",
    "start": "java -jar javagen/target/javagen-jar-with-dependencies.jar",
    "debug": "java -agentlib:jdwp=transport=dt_socket,server=y,suspend=y,quiet=y,address=5005 -jar javagen/target/javagen-jar-with-dependencies.jar",
    "build": "mvn package -Dlocal",
    "generate": "./generate",
    "clean": "rm -rf tests/src/test/java/fixtures"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jianghaolu/autorest.java.v3"
  },
  "readme": "./readme.md",
  "keywords": [
    "autorest",
    "java"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/autorest/issues"
  },
  "homepage": "https://github.com/Azure/autorest.java/blob/master/README.md"
}
