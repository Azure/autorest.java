@using System.Linq;
@using AutoRest.Core.Utilities.Collections;
@using AutoRest.Core.Model
@using AutoRest.Core.Utilities
@using AutoRest.Java
@using AutoRest.Java.Model
@inherits AutoRest.Core.Template<AutoRest.Java.Model.MethodJv>

@{
    var parameterVariants = Model.ParameterVariants;
    var callbackParam = Model.CallbackParam;
}

@if (parameterVariants.HasOptionalParameters)
{
<text>
@* -------------------------------------- *@
@* Synchronous, Body, required parameters *@
@* -------------------------------------- *@
@Model.Javadoc(parameterVariants.RequiredParameters, Model.SyncExceptionDocumentation, Model.SyncReturnDocumentation)
@Model.SyncImpl(parameterVariants.RequiredParameters)
@EmptyLine


@* ----------------------------------- *@
@* Callback, Body, required parameters *@
@* ----------------------------------- *@
@Model.Javadoc(parameterVariants.RequiredParameters.ConcatSingleItem(callbackParam), Model.AsyncExceptionDocumentation, Model.CallbackReturnDocumentation)
@Model.CallbackImpl(parameterVariants.RequiredParameters, callbackParam)
@EmptyLine


@* --------------------------------------------- *@
@* Observable, RestResponse, required parameters *@
@* --------------------------------------------- *@
@Model.Javadoc(parameterVariants.RequiredParameters, Model.AsyncExceptionDocumentation, Model.ObservableReturnDocumentation)
@Model.ObservableRestResponseImpl(parameterVariants.RequiredParameters, takeOnlyRequiredParameters: true)
@EmptyLine


@* ------------------------------------- *@
@* Observable, Body, required parameters *@
@* ------------------------------------- *@
@Model.Javadoc(parameterVariants.RequiredParameters, Model.AsyncExceptionDocumentation, Model.ObservableReturnDocumentation)
@Model.ObservableImpl(parameterVariants.RequiredParameters)
@EmptyLine

</text>
}


@* --------------------------------- *@
@* Synchronous, Body, all parameters *@
@* --------------------------------- *@
@Model.Javadoc(parameterVariants.AllParameters, Model.SyncExceptionDocumentation, Model.SyncReturnDocumentation)
@Model.SyncImpl(parameterVariants.AllParameters)
@EmptyLine


@* ------------------------------ *@
@* Callback, Body, all parameters *@
@* ------------------------------ *@
@Model.Javadoc(parameterVariants.AllParameters.ConcatSingleItem(callbackParam), Model.AsyncExceptionDocumentation, Model.CallbackReturnDocumentation)
@Model.CallbackImpl(parameterVariants.AllParameters, callbackParam)
@EmptyLine


@* ---------------------------------------- *@
@* Observable, RestResponse, all parameters *@
@* ---------------------------------------- *@
@Model.Javadoc(parameterVariants.AllParameters, Model.AsyncExceptionDocumentation, Model.ObservableReturnDocumentation)
@Model.ObservableRestResponseImpl(parameterVariants.AllParameters, takeOnlyRequiredParameters: false)
@EmptyLine


@* -------------------------------- *@
@* Observable, Body, all parameters *@
@* -------------------------------- *@
@Model.Javadoc(parameterVariants.AllParameters, Model.AsyncExceptionDocumentation, Model.ObservableReturnDocumentation)
@Model.ObservableImpl(parameterVariants.AllParameters)
@EmptyLine
