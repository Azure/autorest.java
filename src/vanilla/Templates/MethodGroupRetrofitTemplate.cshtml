@using System.Linq;
@using AutoRest.Core.Model
@using AutoRest.Core.Utilities
@using AutoRest.Java.Model;
@inherits AutoRest.Core.Template<AutoRest.Java.Model.MethodGroupJv>
/**
 * The interface defining all the services for @Model.TypeName to be
 * used by RestProxy to perform REST calls.
*/
@@Host("@(Model.CodeModel.BaseUrl)")
interface @Model.MethodGroupServiceType {
@foreach (MethodJv method in Model.Methods)
{
if (method.RequestContentType == "multipart/form-data" || method.RequestContentType == "application/x-www-form-urlencoded")
{
@:    // @@Multipart not supported by RestProxy
}
else
{
@:    @@Headers({ "x-ms-logging-context: @Model.MethodGroupFullType @method.Name" })
}
@:    @@@(method.HttpMethod.ToString().ToUpper())("@(method.Url.TrimStart('/'))")
if (method.ReturnType.Body.IsPrimaryType(KnownPrimaryType.Stream))
{
@:    // @@Streaming not supported by RestProxy
}
@:    @(method.ExpectedResponsesAnnotation)
if (method.ReturnTypeJv.ReturnValueWireType != null)
{
@:    @@ReturnValueWireType(@(method.ReturnTypeJv.ReturnValueWireType).class)
}
if (method.DefaultResponse.Body != null)
{
@:    @@UnexpectedResponseExceptionType(@(method.OperationExceptionTypeString).class)
}
@:    Single<@(method.RestResponseConcreteTypeName)> @(method.Name)(@method.MethodParameterApiDeclaration);
@EmptyLine
}
}
