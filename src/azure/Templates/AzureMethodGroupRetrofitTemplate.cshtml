@using System.Linq;
@using AutoRest.Core.Model
@using AutoRest.Core.Utilities
@using AutoRest.Java.Azure.Model
@inherits AutoRest.Core.Template<AutoRest.Java.Azure.Model.IMethodGroupJva>
/**
 * The interface defining all the services for @Model.Name to be
 * used by RestProxy to perform REST calls.
 */
@@Host("@(Model.CodeModel.BaseUrl)")
interface @Model.ServiceType {
@foreach (MethodJva method in Model.Methods)
{
@:    @@Headers({ "x-ms-logging-context: @Model.LoggingContext @method.Name" })
if (method.IsPagingNextOperation)
{
@:    @@GET("{nextUrl}")
}
else
{
@:    @@@(method.HttpMethod.ToString().ToUpper())("@(method.Url.TrimStart('/'))")
}
@:    @(method.ExpectedResponsesAnnotation)
if (method.IsLongRunningOperation)
{
@:    Observable<OperationStatus<@(method.ReturnTypeJva.ServiceResponseGenericParameterString)>> @(method.Name)(@method.MethodParameterApiDeclaration);
}
else
{
@:    Single<@(method.ReturnTypeJva.ServiceResponseGenericParameterString)> @(method.Name)(@method.MethodParameterApiDeclaration);
}
@EmptyLine
}
}
