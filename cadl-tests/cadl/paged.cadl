import "@cadl-lang/rest";
import "@azure-tools/cadl-azure-core";
import "@azure-tools/cadl-dpg";

using Cadl.Http;
using Cadl.Rest;
using Azure.Core;
using Azure.DPG;

@service({
  title: "Paged",
  version: "1.0.0",
})
namespace Cadl.Paged;

model Resource {
  @visibility("read")
  id: string;

  @key
  @segment("resources")
  name: string;
  type: string;
}

@route("/paged")
interface PagedOp {
  @convenienceAPI("ListCONVENIENCE")
  list is ResourceList<Resource>;
}
