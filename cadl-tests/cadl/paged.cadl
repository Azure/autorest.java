import "@cadl-lang/rest";
import "@azure-tools/cadl-azure-core";

using Cadl.Http;
using Azure.Core;

@serviceTitle("Paged")
@serviceVersion("1.0.0")
namespace Cadl.Paged;

model Resource {
  id: string;
  name: string;
  type: string;
}

model ResourceCollection is Page<Resource>;

@route("/paged")
interface PagedOp {
  op list(): ResourceCollection;
}
